{
  "name": "python-string-html",
  "displayName": "python-string-html",
  "description": "Syntax highlighting in Python multiline strings",
  "version": "0.0.0",
  "publisher": "ptweir",
  "icon": "docs/logo.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#dedede",
    "theme": "light"
  },
  "repository": {
    "url": "https://github.com/ptweir/python-string-html"
  },
  "bugs": {
    "url": "https://github.com/ptweir/python-string-html/issues"
  },
  "main": "./scripts/main",
  "categories": [
    "Languages"
  ],
  "keywords": [
    "python",
    "python3",
    "python-string-html",
    "html",
    "sql"
  ],
  "activationEvents": [
    "onCommand:pythonstringhtml.insertComment",
    "onCommand:pythonstringhtml.insertTemplate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pythonstringhtml.insertComment",
        "title": "Insert python-string-html comment",
        "category": "HTML"
      },
      {
        "command": "pythonstringhtml.insertTemplate",
        "title": "Insert python-string-html template",
        "category": "HTML"
      }
    ],
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.sql",
          "text.html.basic"
        ],
        "scopeName": "inline.es6-js-injection",
        "path": "./syntaxes/es6-js-injection.json",
        "embeddedLanguages": {
          "meta.template.expression.ts": "typescript",
          "meta.template.expression.js": "javascript"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "text.html.basic"
        ],
        "scopeName": "inline.es6-html",
        "path": "./syntaxes/es6-inline-html.json",
        "embeddedLanguages": {
          "meta.embedded.block.html": "html"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.sql"
        ],
        "scopeName": "inline.es6-sql",
        "path": "./syntaxes/es6-inline-sql.json",
        "embeddedLanguages": {
          "meta.embedded.sql": "sql"
        }
      }
    ]
  }
}
